#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/okhaeeun/Desktop/elo-community/elo-community-backend-nest/contracts/node_modules/.pnpm/@openzeppelin+hardhat-upgrades@3.9.1_@nomicfoundation+hardhat-ethers@3.1.0_ethers@6.15._432a62e1621259f4b626a5cc33fde33a/node_modules/@openzeppelin/hardhat-upgrades/dist/scripts/node_modules:/Users/okhaeeun/Desktop/elo-community/elo-community-backend-nest/contracts/node_modules/.pnpm/@openzeppelin+hardhat-upgrades@3.9.1_@nomicfoundation+hardhat-ethers@3.1.0_ethers@6.15._432a62e1621259f4b626a5cc33fde33a/node_modules/@openzeppelin/hardhat-upgrades/dist/node_modules:/Users/okhaeeun/Desktop/elo-community/elo-community-backend-nest/contracts/node_modules/.pnpm/@openzeppelin+hardhat-upgrades@3.9.1_@nomicfoundation+hardhat-ethers@3.1.0_ethers@6.15._432a62e1621259f4b626a5cc33fde33a/node_modules/@openzeppelin/hardhat-upgrades/node_modules:/Users/okhaeeun/Desktop/elo-community/elo-community-backend-nest/contracts/node_modules/.pnpm/@openzeppelin+hardhat-upgrades@3.9.1_@nomicfoundation+hardhat-ethers@3.1.0_ethers@6.15._432a62e1621259f4b626a5cc33fde33a/node_modules/@openzeppelin/node_modules:/Users/okhaeeun/Desktop/elo-community/elo-community-backend-nest/contracts/node_modules/.pnpm/@openzeppelin+hardhat-upgrades@3.9.1_@nomicfoundation+hardhat-ethers@3.1.0_ethers@6.15._432a62e1621259f4b626a5cc33fde33a/node_modules:/Users/okhaeeun/Desktop/elo-community/elo-community-backend-nest/contracts/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/okhaeeun/Desktop/elo-community/elo-community-backend-nest/contracts/node_modules/.pnpm/@openzeppelin+hardhat-upgrades@3.9.1_@nomicfoundation+hardhat-ethers@3.1.0_ethers@6.15._432a62e1621259f4b626a5cc33fde33a/node_modules/@openzeppelin/hardhat-upgrades/dist/scripts/node_modules:/Users/okhaeeun/Desktop/elo-community/elo-community-backend-nest/contracts/node_modules/.pnpm/@openzeppelin+hardhat-upgrades@3.9.1_@nomicfoundation+hardhat-ethers@3.1.0_ethers@6.15._432a62e1621259f4b626a5cc33fde33a/node_modules/@openzeppelin/hardhat-upgrades/dist/node_modules:/Users/okhaeeun/Desktop/elo-community/elo-community-backend-nest/contracts/node_modules/.pnpm/@openzeppelin+hardhat-upgrades@3.9.1_@nomicfoundation+hardhat-ethers@3.1.0_ethers@6.15._432a62e1621259f4b626a5cc33fde33a/node_modules/@openzeppelin/hardhat-upgrades/node_modules:/Users/okhaeeun/Desktop/elo-community/elo-community-backend-nest/contracts/node_modules/.pnpm/@openzeppelin+hardhat-upgrades@3.9.1_@nomicfoundation+hardhat-ethers@3.1.0_ethers@6.15._432a62e1621259f4b626a5cc33fde33a/node_modules/@openzeppelin/node_modules:/Users/okhaeeun/Desktop/elo-community/elo-community-backend-nest/contracts/node_modules/.pnpm/@openzeppelin+hardhat-upgrades@3.9.1_@nomicfoundation+hardhat-ethers@3.1.0_ethers@6.15._432a62e1621259f4b626a5cc33fde33a/node_modules:/Users/okhaeeun/Desktop/elo-community/elo-community-backend-nest/contracts/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@openzeppelin/hardhat-upgrades/dist/scripts/migrate-oz-cli-project.js" "$@"
else
  exec node  "$basedir/../@openzeppelin/hardhat-upgrades/dist/scripts/migrate-oz-cli-project.js" "$@"
fi

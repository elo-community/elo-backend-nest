#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/okhaeeun/Desktop/elo-community/elo-community-backend-nest/contracts/node_modules/.pnpm/@openzeppelin+upgrades-core@1.44.1/node_modules/@openzeppelin/upgrades-core/dist/cli/node_modules:/Users/okhaeeun/Desktop/elo-community/elo-community-backend-nest/contracts/node_modules/.pnpm/@openzeppelin+upgrades-core@1.44.1/node_modules/@openzeppelin/upgrades-core/dist/node_modules:/Users/okhaeeun/Desktop/elo-community/elo-community-backend-nest/contracts/node_modules/.pnpm/@openzeppelin+upgrades-core@1.44.1/node_modules/@openzeppelin/upgrades-core/node_modules:/Users/okhaeeun/Desktop/elo-community/elo-community-backend-nest/contracts/node_modules/.pnpm/@openzeppelin+upgrades-core@1.44.1/node_modules/@openzeppelin/node_modules:/Users/okhaeeun/Desktop/elo-community/elo-community-backend-nest/contracts/node_modules/.pnpm/@openzeppelin+upgrades-core@1.44.1/node_modules:/Users/okhaeeun/Desktop/elo-community/elo-community-backend-nest/contracts/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/okhaeeun/Desktop/elo-community/elo-community-backend-nest/contracts/node_modules/.pnpm/@openzeppelin+upgrades-core@1.44.1/node_modules/@openzeppelin/upgrades-core/dist/cli/node_modules:/Users/okhaeeun/Desktop/elo-community/elo-community-backend-nest/contracts/node_modules/.pnpm/@openzeppelin+upgrades-core@1.44.1/node_modules/@openzeppelin/upgrades-core/dist/node_modules:/Users/okhaeeun/Desktop/elo-community/elo-community-backend-nest/contracts/node_modules/.pnpm/@openzeppelin+upgrades-core@1.44.1/node_modules/@openzeppelin/upgrades-core/node_modules:/Users/okhaeeun/Desktop/elo-community/elo-community-backend-nest/contracts/node_modules/.pnpm/@openzeppelin+upgrades-core@1.44.1/node_modules/@openzeppelin/node_modules:/Users/okhaeeun/Desktop/elo-community/elo-community-backend-nest/contracts/node_modules/.pnpm/@openzeppelin+upgrades-core@1.44.1/node_modules:/Users/okhaeeun/Desktop/elo-community/elo-community-backend-nest/contracts/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../upgrades-core/dist/cli/cli.js" "$@"
else
  exec node  "$basedir/../../../upgrades-core/dist/cli/cli.js" "$@"
fi
